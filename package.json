{
  "name": "healthfuel365-backend",
  "version": "1.0.0",
  "description": "node backend for healthfuel365",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --ext js,ts --fix src/",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest --passWithNoTests",
    "postinstall": "npm run build"
  },
  "author": "HealthFuel365",
  "license": "MIT",
  "dependencies": {
    "@azure/ai-form-recognizer": "^3.2.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^24.0.22",
    "@types/mongodb": "^4.0.7",
    "@types/swagger-ui-express": "^4.1.3",
    "axios": "^0.21.4",
    "bcrypt": "^5.0.1",
    "bignumber.js": "^9.1.1",
    "body-parser": "^1.19.1",
    "connect": "^3.7.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-form-data": "^2.0.16",
    "form-data": "^4.0.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.4.1",
    "mongoose": "^7.2.0",
    "regex": "^0.1.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.1",
    "@types/express-form-data": "^2.0.1",
    "@types/jquery": "^3.5.14",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/mongoose": "^5.11.97",
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-typescript": "^6.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "jest": "^24.9.0",
    "jest-watch-typeahead": "^0.4.0",
    "nodemon": "^1.19.4",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.7.4"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,ts}",
      "!src/**/*.d.ts"
    ],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,ts}",
      "<rootDir>/src/**/*.{spec,test}.{js,ts}"
    ],
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|ts)$"
    ],
    "modulePaths": [],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "json",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  }
}
